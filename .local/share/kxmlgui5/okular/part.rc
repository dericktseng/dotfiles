  <Action name="first_page" shortcut="G"/>
  <Action name="fit_window_to_page" shortcut=""/>
  <Action name="go_document_back" shortcut="Ctrl+O"/>
  <Action name="go_document_forward" shortcut="Ctrl+I"/>
  <Action name="go_next" shortcut="Space"/>
  <Action name="go_previous" shortcut="Shift+Space; Backspace"/>
  <Action name="last_page" shortcut="Shift+G"/>
  <Action name="mouse_textselect" shortcut="Ctrl+2"/>
  <Action name="mouse_zoom" shortcut=""/>
  <Action name="next_bookmark" shortcut="N"/>
  <Action name="previous_bookmark" shortcut="Shift+N"/>
  <Action name="show_bottombar" shortcut="Ctrl+N"/>
  <Action name="show_leftpanel" priority="0" shortcut="Tab"/>
  <Action name="view_actual_size" shortcut="0"/>
  <Action name="view_fit_to_page" shortcut="A"/>
  <Action name="view_fit_to_width" shortcut="W"/>
  <Action name="view_orientation_rotate_ccw" shortcut="Shift+R"/>
